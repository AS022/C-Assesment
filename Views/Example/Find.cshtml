@model CoustmerApp.Models.CustTable



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Find</title>
</head>
<body>
    <h1>Details of Customer to Update</h1>
    <div>
        @using (Html.BeginForm())
        {
            <p>
                Enter the CustID: @Html.EditorFor((m) => m.CustID)
            </p>
            <p>
                Enter the CustName: @Html.EditorFor((m) => m.CustName)
            </p>
            <p>
                Enter the CustAddress: @Html.EditorFor((m) => m.CustAddress)
            </p>
            <button>Save Changes</button>
        }
    </div>

    
</body>
</html>
